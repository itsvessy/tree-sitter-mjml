{
  "metadata": {
    "version": "0.0.1",
    "description": "MJML grammar for tree-sitter (based on tree-sitter-xml)",
    "links": {
      "repository": ""
    },
    "authors": [
      {
        "name": "Vessy Stroumsky",
        "email": "",
        "url": ""
      }
    ]
  },
  "grammars": [
    {
      "name": "mjml",
      "camelcase": "MJML",
      "scope": "source.mjml",
      "file-types": [
        "mjml",
        "mjml.html"
      ],
      "injection-regex": "^(mjml|mjml\\.html)$",
      "highlights": "queries/mjml/highlights.scm",
      "path": "xml",
      "external-files": [
        "common/scanner.h"
      ]
    }
  ],
  "bindings": {
    "go": false,
    "swift": false
  }
}
